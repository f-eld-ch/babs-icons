    "cpx": "^1.5.0",
    "svg-to-ts": "^12.0.0",
    "vite": "^5.4.12"
  },
  "resolutions": {
    "braces": "^3.0.3",
    "ip": "^2.0.1",
    "tar": "^6.2.1",
    "micromatch": "^4.0.8",
    "rollup": "^4.22.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/f-eld-ch/babs-icons.git"
  },
  "keywords": [
    "SVG",
    "icons",
    "library",
    "starter"
  ],
